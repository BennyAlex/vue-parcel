<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width,initial-scale=1.0" />

	{{!-- <meta name="author" content="" />
	<meta name="description" content="" /> --}}
	<meta name="robots" content="index,follow" />

	<title>Static Webapp</title>

	<link rel="icon" type="image/png" href="images/favicons/favicon.png" />
	<link rel="apple-touch-icon" href="images/favicons/apple-touch-icon.png" />

	{{! windows platform }}
	<meta name="application-name" content="Static Webapp" />
    <meta name="msapplication-TileColor" content="#efefef" />
    <meta name="msapplication-TileImage" content="images/favicons/mstile.png" />

	<!-- build:css assets/app.min.css -->
	<link rel="stylesheet" type="text/css" href="assets/app.css">
	<!-- endbuild -->

</head>
<body>

	{{! app wrapper }}
	<div id="app">

		{{> header }}

        {{> body }}

		{{> footer }}

	</div>

	<!-- build:js assets/app.min.js -->
	<script type="text/javascript" src="assets/app.js"></script>
	<!-- endbuild -->

	<noscript>
        This site requires Javascript enabled in your browser.
	</noscript>

